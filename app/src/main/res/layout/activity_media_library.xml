<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/media_library"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MediaLibraryActivity">

    <LinearLayout
        style="@style/otherActivityLinearLayoutStyle"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout style="@style/HeaderLinearLayoutStyle">

            <FrameLayout
                android:id="@+id/return_frame"
                style="@style/HeaderImageFrameStyle">

                <ImageView style="@style/ArrowBackImageViewStyle" />

            </FrameLayout>

            <FrameLayout style="@style/HeaderFrameStyle">

                <TextView
                    style="@style/HeaderTextViewStyle"
                    android:text="@string/media_library_title" />

            </FrameLayout>

        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>